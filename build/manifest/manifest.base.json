{
  "name": "Web3 NFT OpenSea",
  "author": "web3",
  "version": "0.0.1",
  "description": "Web3 NFT Description",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "permissions": ["storage", "downloads", "tabs"],
  "optional_permissions": ["unlimitedStorage", "downloads"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_title": "Web3 NFT",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png"
    }
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "js": ["js/baidu.js"],
      "matches": ["https://www.baidu.com/*", "http://www.baidu.com/*"],
      "run_at": "document_start"
    },
    {
      "js": ["js/google.js"],
      "matches": ["http://*/*", "https://*/*"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["images/*.png"],
      "matches": [],
      "extension_ids": []
    }
  ]
}
